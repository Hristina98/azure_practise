{
	"name": "failure_test",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "CollegeRanks"
				},
				{
					"dataset": {
						"referenceName": "numOfStud",
						"type": "DatasetReference"
					},
					"name": "NumOfStud"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "split1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> CollegeRanks",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NumOfStud",
				"CollegeRanks split(equals(\"CollegeName\", \"\"),",
				"     disjoint: false) ~> split1@(Stream1, Other)",
				"split1@Other filter(\"CollegeName\" == \"Nesto\") ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> sink1"
			]
		}
	}
}